        <% product.rawMRS.forEach((item,i) => { %>
        <tr>
          <td><%= item.slNo || i+1 %></td>    
          <td><%= item.materialCode %></td>
          <td><%= item.materialName %></td>
          <td><%= item.ARNo %></td>
          <td><%= item.reasonOfRejection %></td>
          <td><%= item.description %></td>
        </tr>
        <% }); %>
