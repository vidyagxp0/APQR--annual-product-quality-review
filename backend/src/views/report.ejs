      <tr>
        <td><%= obj.productName %></td>
        <td><%= obj.sFGCode %></td>
        <td><%= obj.fGCode %></td>
      </tr>
      <% }); %>
    </table>

      <div class="summary">
      <h3>Manufacturing Site Address Summary :-</h3>
      <!-- &nbsp &nbsp -->
      <p><%= product.msaSummary %></p>
    </div>

      <h3>Manufacturing Site Address 2</h3>
